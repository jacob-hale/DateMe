@{
}

@model List<Application>

<table class="fw-bold">
    <thead>
        <tr>
        <td>First name</td>
        <td>Last name</td>
        <td>Age</td>
        <td>Phone</td>
        <td>Major</td>
        <td>Creeper and/or Stalker</td>
        </tr>
    </thead>

    @foreach (var a in Model){
    <tbody>
        <tr>
        <td>@a.FirstName</td>
        <td>@a.LastName</td>
        <td>@a.Age</td>
        <td>@a.PhoneNumber</td>
        <td>@a.Major</td>
        <td>@a.CreeperStalker</td>
            </tr>
    </tbody>
    }
</table>